{
    "$schema": "https://railway.com/railway.schema.json",
    "build": {
      "builder": "DOCKERFILE",
      "dockerfilePath": "core/anything-server/Dockerfile"
    },
    "deploy": {
      "startCommand": "/app/anything-server",
      "healthcheckPath": "/",
      "healthcheckTimeout": 300,
      "restartPolicyType": "ON_FAILURE"
    }
  }